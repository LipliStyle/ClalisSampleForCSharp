    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ClalisSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="ClalisSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://liplis.mine.nu/Clalis/v30/Soap/Clalis.asmx"
